//https://codeforces.com/problemset/problem/723/A

#include<bits\stdc++.h>
using namespace std;

int main(){
    int x1,x2,x3;
    cin>>x1>>x2>>x3;
    vector<int> v;
    v.push_back(x1);
    v.push_back(x2);
    v.push_back(x3);
    sort(v.begin(),v.end());
    int y1= v[1]-v[0];
    int y2=v[2]-v[1];
    int res= y1+y2;
    cout<<res;
}
